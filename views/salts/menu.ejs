<% layout('layouts/template') %>

<div class="d-flex flex-wrap mt-5">
    <ul>
    <% for(let c of starter) { %>
        <li>
           <h3><%= c.name %></h3>
        </li>
        <hr id="hr">
        <% for(let i = 0; i < c.dishes.length; i++) {%>
            
            <div class="card" id="card" style="width: 18rem;">
                <img src="https://images.unsplash.com/photo-1625938144755-652e08e359b7?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxzZWFyY2h8NXx8c3RhcnRlcnN8ZW58MHx8MHx8&auto=format&fit=crop&w=700&q=60" class="card-img-top" alt="...">
                <div class="card-body">
                    <h3><%= c.dishes[i].food%></h3>
                    <p><%= c.dishes[i].details%></p>
                    <p><%= c.dishes[i].price%></p>      
                </div>
            </div>
        <%}%>     
    <% } %>
    </ul>
